source ~/.config/lf/key-functions

# default shell
set shell zsh

# delete
map d delete

# copy
map c copy

# cut
map x cut

# show hidden file by default
set hidden

# Bind 'a' to newfile command
map a newfile

# Bind 'A' to newdir command
map A newdir

# preview imgs
#set previewer ~/.config/lf/pv
